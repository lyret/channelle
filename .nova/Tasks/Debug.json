{
  "actions" : {
    "build" : {
      "postActions" : [
        {
          "action" : "build",
          "taskName" : "Dev",
          "type" : "runTask"
        }
      ]
    }
  },
  "buildBeforeRunning" : true,
  "extension" : {
    "identifier" : "panic.JavaScript",
    "name" : "JavaScript"
  },
  "extensionTemplate" : "nodeDebug",
  "extensionValues" : {
    "launchArgs" : [
      "run:debug",
      "--enable-source-maps"
    ],
    "request" : "launch",
    "scriptPath" : "cli.mjs"
  },
  "openLogOnRun" : "fail"
}
