{
  "trailingComma": "es5",
  "tabWidth": 2,
  "useTabs": true,
  "semi": true,
  "singleQuote": true,
  "plugins": ["prettier-plugin-svelte", "prettier-plugin-prisma"],
  "overrides": [
    { "files": "*.svelte", "options": { "parser": "svelte" } },
    { "files": "*.prisma", "options": { "parser": "prisma" } }
  ]
}
