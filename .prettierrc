{
	
	"trailingComma": "es5",
	"tabWidth": 2,
	"useTabs": true,
	"semi": true,
	"singleQuote": true,
	"quoteProps": "as-needed",
	"bracketSpacing": true,
	"bracketSameLine": false,
	"arrowParens": "always",
	"proseWrap": "preserve",
	"htmlWhitespaceSensitivity": "css",
	"embeddedLanguageFormatting": "auto",
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-prisma"],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}