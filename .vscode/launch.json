{
	"compounds": [
		{
			"name": "Server/Client",
			"stopAll": true,
			"configurations": ["Server", "Client"]
		}
	],
	"configurations": [
		{
			"name": "Theather",
			"request": "launch",
			"runtimeArgs": ["run-script", "theater:dev"],
			"runtimeExecutable": "npm",
			"skipFiles": ["<node_internals>/**"],
			"type": "node"
		},
		{
			"name": "Stage",
			"request": "launch",
			"runtimeArgs": ["run-script", "stage:dev"],
			"runtimeExecutable": "npm",
			"skipFiles": ["<node_internals>/**"],
			"type": "node"
		},
		{
			"type": "chrome",
			"name": "Client",
			"request": "launch",
			"webRoot": "${workspaceFolder}/.dist/ui",
			"url": "http://localhost:3000"
		}
	]
}
